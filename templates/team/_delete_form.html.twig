<form method="post" action="{{ path('team_delete', {'id': team.id}) }}" onsubmit="return confirm('Czy jesteś pewien że chcesz usunąć tą drużynę ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ team.id) }}">
    <button class="btn">Usuń</button>
</form>
